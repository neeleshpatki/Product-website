<%- include('menu')%>
<div class="container">
  <h1>Show User</h1>

  <table class="table">
    <thead>
      <tr>
      
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Mobile No</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(function(value){%>
      <tr>
       
        <td><%=value.name%></td>
        <td><%=value.email%></td>
        <td><%=value.mobile%></td>
      </tr>
      <%})%>
    </tbody>
  </table>
</div>
